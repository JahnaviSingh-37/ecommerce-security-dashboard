{
  "name": "ecommerce-security-backend",
  "version": "1.0.0",
  "description": "Backend API for E-Commerce Security Dashboard",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --coverage",
    "migrate": "ts-node src/database/migrate.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
